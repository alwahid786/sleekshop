{% import "part_tpl_vars.twig" as tpl_vars %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ res.title }}</title>
    <!-- Latest compiled and minified CSS -->
  
    {% include "part_favicon.twig" ignore missing %}
    <noscript>
        <link rel="stylesheet" href="/css/bootstrap/bootstrap.min.css">
    </noscript>
    
    {%if canonical!=''%}<link rel="canonical" href="{{canonical}}" />{%endif%}
    <base href="/">
    	<link rel="stylesheet" href="assets/css/lightbox.css">
	<link rel="stylesheet" type="text/css" href="assets/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.css">
	<link rel="stylesheet" type="text/css" href="assets/css/style.css">
	<link rel="stylesheet" type="text/css" href="assets/css/fonts.css">
	<link rel="stylesheet" type="text/css" href="assets/css/responsive.css">
	<link rel="stylesheet" type="text/css" href="assets/css/helena-custom.css">
    {% include "part_theming.twig" ignore missing %}
    {% include "part_header_snippets.twig" ignore missing %}
</head>
<body>
{% for value in res.contents.byclass.shop_banner %}
    <div class="alert alert-{{ value.attributes.color.value }} text-center" role="alert" style="top: 56px;">
        {{ value.attributes.text.value }}
    </div>
{% endfor %}

{% block header %}

   <header id="header" class="transparent-header">
	<nav class="navbar navbar-expand-lg">
	    <div class="container-fluid">
	    	<a class="navbar-brand" href="/">
	           {% include 'part_logo.twig' ignore missing %}
               </a>
	        <div class="navbar-collapse collapse dual-collapse2">
	           <ul class="navbar-nav me-auto">
                <li class="nav-item"><a class="nav-link" href="/shop">Shop</a></li>
                <li class="nav-item"><a class="nav-link" href="/stories">Stories</a></li>
                <li class="nav-item"><a class="nav-link" href="/collection">Collection</a></li>
                <li class="nav-item"><a class="nav-link" href="/assistant">Assistant</a></li>
                <li class="nav-item"><a class="nav-link" href="/faqs">Faq's</a></li>
                <li class="nav-item"><a class="nav-link" href="/contact-us">Contact Us</a></li>
            </ul>

	        </div>
	       <div class="header-right">
		       	<ul class="header-info-box">
		        	{% include 'part_social_links.twig' ignore missing %}
		        	<li class="search-bt"><a href="javascript:void(0)"><img src="assets/images/search-icon.svg"></a></li>
		        	<li class="nav-item dropdown">
						<a href="#" class="nav-link dropdown-toggle" id="userDropdown" data-bs-toggle="dropdown" aria-expanded="false">
							<img src="assets/images/user-icon.svg" alt="User Icon">
						</a>
						<ul class="dropdown-menu dropdown-menu-end" aria-labelledby="userDropdown">
							{% if username is defined and username != '' %}
								<li><a class="dropdown-item ms-0" href="/account">üë§ {{ username }}</a></li>
								<li><a class="dropdown-item ms-0" href="/logout">üö™ Logout</a></li>
							{% else %}
								<li><a class="dropdown-item ms-0" href="/login">üîê Login</a></li>
								<li><a class="dropdown-item ms-0" href="/register">üìù Register</a></li>
							{% endif %}
						</ul>
					</li>

		        	<li><a class="cart-option" id="cart-option" href="javascript:void(0)">
		        		<img src="assets/images/cart-icon.svg">
		        		<span class="cart-count">{{ cart.contents|length }}</span>
		        	</a></li>
		        </ul>
		        <button class="navbar-toggler" onclick="myFunction(this)" type="button" data-bs-toggle="collapse" data-bs-target=".dual-collapse2">
		        	<div class="bar1"></div>
  					<div class="bar2"></div>
  					<div class="bar3"></div>
		        </button>
	       </div>
	    </div>
	</nav>
</header>
<section class="search-wrapper">
	<a class="close-bt" href="javascript:void(0)"><img src="assets/images/close.png"></a>
	<div class="container">	
		<div class="row">
			<div class="col-md-12">
				<form>
					<div class="form-group">
						<input type="search" name="Search"  value="{% trans %}Search{% endtrans %}" placeholder="Search here..">
					</div>
				</form> 
			</div>
		</div>
	</div>
</section>

{% endblock %}
{% block head %}

{% endblock %}

{% block content %} HERE  OUR CONTENT {% endblock %}

{% block footer %}
<footer id="footer">
	<div class="container footer-top-row varticle-arrow">
		<div class="row">
			<div class="col-md-6 col-lg-4 col-xl-4 col-12 footer-left-border order-2 order-lg-1 mt-5 mt-lg-0">
				<h4 class="footer-title text-uppercase">DISCOVER</h4>
				 {% for item in menu %}
                    {% if item.name == 'footer' %}
                        {% for item in item.children %}
                            <a href="{% if item.link != "" %}{{ item.link }}{% else %}{% if item.permalink!='' %}/{{ item.permalink }}{% else %}/{{ item.id }}{% endif %}{% endif %}">{{ item.label }}</a>
                            <br>
                        {% endfor %}
                    {% endif %}
                {% endfor %}
			</div>
			<div class="col-lg-4 col-xl-4 col-12 footer-middle-border order-1 order-lg-2">
				<figure class="text-center footer-logo"><a href="index.html">  {% include 'part_logo.twig' ignore missing %}</a></figure>
				  {% include 'part_about_us_footer_'~language|slice(0,2) ~ '.twig' ignore missing %}
				<ul class="list-group list-group-horizontal footer-social justify-content-center">
					 {% include 'part_social_links.twig' ignore missing  %}
				</ul>
			</div>
			<div class="col-md-6 col-lg-4 col-xl-4 col-12 footer-right-border order-3 order-lg-3 mt-5 mt-lg-0">
				<h4 class="footer-title text-uppercase text-md-end text-start">HELP & INFORMATION</h4>
				<ul class="list-group footer-list text-md-end text-start">
				  <li class="list-group-item">CALL US : <a href="tel:+123456789">+123-456-789</a></li>
				  <li class="list-group-item"><a href="mailto:info@example.com">INFO@EXAMPLE.COM</a></li>
				  <li class="list-group-item">MONDAY TILL FRIDAY 10 TO 6 EST</li>
				</ul>
			</div>
		</div>
	</div>
	<div class="footer-bottom-row">
		<div class="container">
			<div class="row">
			<div class="col-12 col-lg-6">
				<p class="copyright-text mt-0 mb-3 text-center text-lg-start mb-lg-0 lh-lg">Copyright ¬© 2022 Helena. All Rights Reserved.</p>
			</div>
			<div class="col-12 col-lg-6">
				<ul class="list-group list-group-horizontal footer-list justify-content-center justify-content-lg-end">
				  <li class="list-group-item"><a href="#">Privacy Policy</a></li>
				  <li class="list-group-item"><a href="#">Terms & Conditions</a></li>
				</ul>
			</div>				
			</div>
		</div>
	</div>
</footer>
{% include "part_cart_sidebar.twig" ignore missing %}
{% endblock %}


</body>
<script src="https://code.jquery.com/jquery-3.6.0.js"></script>
<script src="assets/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
<script src="assets/js/custom.js"></script>
{% include "part_bottom_snippets.twig" ignore missing%}
</html>
